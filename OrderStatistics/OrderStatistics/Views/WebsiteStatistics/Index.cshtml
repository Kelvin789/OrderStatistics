@model IEnumerable<OrderStatistics.Models.Website>

@{
    ViewBag.Title = "Website Statistics";
}

<h2>Website Statistics</h2>

<p>
    @Html.ActionLink("Create New", "Create", "WebsiteStatistics", null, new { @class = "navigate-links" })
</p>
<table class="table">
    <tr>
        <td>
            @Html.DisplayNameFor(model => model.WeekId)
        </td>
        <th>
            @Html.DisplayNameFor(model => model.PageLoadTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UniqueLogins)
        </th>
        <th></th>
    </tr>

    @if (Model == null)
    {
        <span>no items found</span>
    }
    else
    {
        foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.WeekId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PageLoadTime)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UniqueLogins)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", "WebsiteStatistics", new { id = item.Id }, new { @class = "navigate-links" }) |
                    @Html.ActionLink("Details", "Details", "WebsiteStatistics", new { id = item.Id }, new { @class = "navigate-links" }) |
                    @Html.ActionLink("Delete", "Delete", "WebsiteStatistics", new { id = item.Id }, new { @class = "navigate-links" })
                </td>
            </tr>
        }
    }
</table>

<div>
    @Html.ActionLink("Back to Home Page", "Index", "Home", null, new { @class = "navigate-links" })
</div>